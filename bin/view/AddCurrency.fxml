<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminCurrencyController">
   <children>
      <Label fx:id="labelCode" layoutX="90.0" layoutY="101.0" text="Currency Code" textFill="#13459a">
         <effect>
            <Bloom>
               <input>
                  <Blend />
               </input>
            </Bloom>
         </effect>
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="labelcountry" layoutX="131.0" layoutY="140.0" text="Country" textFill="#13459a">
         <effect>
            <Bloom>
               <input>
                  <Blend />
               </input>
            </Bloom>
         </effect>
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="CurrencyCode" layoutX="195.0" layoutY="95.0" onAction="#OnAddCurrencyClick" />
      <TextField fx:id="Country" layoutX="194.0" layoutY="134.0" onAction="#OnAddCurrencyClick" />
      <Button fx:id="AddCurrency" layoutX="205.0" layoutY="253.0" mnemonicParsing="false" onAction="#OnAddCurrencyClick" prefHeight="31.0" prefWidth="51.0" text="Add" />
      <Button fx:id="AdminHomeButton" layoutX="270.0" layoutY="253.0" mnemonicParsing="false" onAction="#onAdminHomeClick" text="Back" />
      <Label layoutX="201.0" layoutY="31.0" text="Add Currency" textFill="#13459a">
         <font>
            <Font size="24.0" />
         </font>
         <effect>
            <Bloom>
               <input>
                  <Blend />
               </input>
            </Bloom>
         </effect>
      </Label>
      <Label fx:id="labelvalue" layoutX="90.0" layoutY="179.0" text="Currency Value" textFill="#13459a">
         <effect>
            <Bloom>
               <input>
                  <Blend />
               </input>
            </Bloom>
         </effect>
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="CurrencyValue" layoutX="195.0" layoutY="173.0" onAction="#OnAddCurrencyClick" />
   </children>
</AnchorPane>
